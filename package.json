{
	"name": "wok-web",
	"version": "0.0.1",
	"scripts": {
		"start": "astro dev",
		"build": "astro build && npm run ucompress && npm run minify-html",
		"ucompress": "ucompress --dest ./dist --source ./dist",
		"minify-html": "html-minifier -c html-minifier.config.json --input-dir dist/ --output-dir dist/ --file-ext html"
	},
	"devDependencies": {
		"@tailwindcss/aspect-ratio": "0.4.0",
		"@tailwindcss/forms": "0.4.0",
		"@tailwindcss/typography": "0.5.0",
		"astro": "0.22.3",
		"cssnano": "5.0.14",
		"cssnano-preset-advanced": "5.1.9",
		"firebase": "9.6.1",
		"html-minifier": "4.0.0",
		"postcss-combine-duplicated-selectors": "10.0.3",
		"postcss-combine-media-query": "1.0.1",
		"postcss-import": "14.0.2",
		"postcss-url": "10.1.3",
		"snowpack": "3.8.8",
		"tailwindcss": "3.0.9",
		"ucompress": "0.22.2"
	}
}
