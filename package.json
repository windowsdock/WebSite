{
	"name": "wok-web",
	"version": "0.0.1",
	"scripts": {
		"start": "astro dev",
		"build": "astro build && npm run ucompress && npm run minify-html && npm run minify-firebase && npm run minify-webfontloader",
		"ucompress": "ucompress --dest ./dist --source ./dist && ucompress --dest ./dist/_astro --source ./dist/_astro",
		"minify-html": "html-minifier -c html-minifier.config.json --input-dir dist/ --output-dir dist/ --file-ext html",
		"minify-firebase": "terser dist/_astro/src/assets/js/firebase.js --config-file terser.config.json -o dist/_astro/src/assets/js/firebase.js",
		"minify-webfontloader": "terser dist/_astro/src/assets/js/webfontloader.js --config-file terser.config.json -o dist/_astro/src/assets/js/webfontloader.js"
	},
	"devDependencies": {
		"@snowpack/plugin-postcss": "1.4.3",
		"@tailwindcss/ui": "0.7.2",
		"astro": "0.20.8",
		"cssnano": "5.0.8",
		"cssnano-preset-advanced": "5.1.4",
		"firebase": "9.1.1",
		"html-minifier": "4.0.0",
		"postcss-combine-duplicated-selectors": "10.0.3",
		"postcss-combine-media-query": "1.0.1",
		"postcss-import": "14.0.2",
		"postcss-url": "10.1.3",
		"snowpack": "3.8.8",
		"tailwindcss": "2.2.16",
		"terser": "5.9.0",
		"ucompress": "0.22.2"
	}
}
