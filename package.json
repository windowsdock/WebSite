{
	"name": "wok-web",
	"version": "0.0.1",
	"scripts": {
		"build": "astro build && npm run ucompress && npm run minify-html",
		"minify-html": "html-minifier -c .html-minifier.json --input-dir dist/ --output-dir dist/ --file-ext html",
		"start": "astro dev",
		"ucompress": "ucompress --dest ./dist --source ./dist"
	},
	"devDependencies": {
		"@tailwindcss/aspect-ratio": "0.4.0",
		"@tailwindcss/forms": "0.5.0",
		"@tailwindcss/line-clamp": "0.3.1",
		"@tailwindcss/nesting": "0.0.0-insiders.565cd3e",
		"@tailwindcss/typography": "0.5.2",
		"astro": "0.24.3",
		"autoprefixer": "10.4.4",
		"cssnano": "5.1.5",
		"cssnano-preset-advanced": "5.3.1",
		"firebase": "9.6.9",
		"html-minifier": "4.0.0",
		"postcss": "8.4.12",
		"postcss-combine-duplicated-selectors": "10.0.3",
		"postcss-combine-media-query": "1.0.1",
		"postcss-import": "14.0.2",
		"postcss-reporter": "7.0.5",
		"postcss-url": "10.1.3",
		"snowpack": "3.8.8",
		"tailwindcss": "3.0.23",
		"ucompress": "0.22.2"
	}
}
